<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Project.
    -->
    <changeSet id="00000000000010" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="projects_id"
                                 baseTableName="project_device_type"
                                 constraintName="fk_project_device_type_projects_id"
                                 referencedColumnNames="id"
                                 referencedTableName="project"/>
        <addForeignKeyConstraint baseColumnNames="device_types_id"
                                 baseTableName="project_device_type"
                                 constraintName="fk_project_device_type_device_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="device_type"/>
        <addForeignKeyConstraint baseColumnNames="project_admin"
                                baseTableName="project"
                                constraintName="fk_project_owner_id"
                                referencedColumnNames="id"
                                referencedTableName="radar_user"/>

    </changeSet>
</databaseChangeLog>
